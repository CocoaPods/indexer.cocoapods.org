{
  "name": "indexer.cocoapods.org",
  "description": "",
  "private": true,
  "scripts": {
    "build": "yarn clean:build && babel src --ignore src/**/__tests__ --out-dir dist --extensions '.ts,.js'",
    "dev": "babel-node --extensions '.ts,.tsx' src/index.ts | bunyan",
    "start": "node dist/index.js | bunyan",
    "clean:build": "rm -rf dist",
    "coverage": "jest --coverage",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "test:watch": "jest -o --watch",
    "typecheck": "tsc"
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ],
    "moduleDirectories": [
      "./node_modules",
      "./src"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "ts-jest"
    }
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/node": "7.0.0-beta.46",
    "@babel/preset-env": "7.0.0-beta.46",
    "@babel/preset-stage-3": "7.0.0-beta.46",
    "@babel/preset-typescript": "7.0.0-beta.46",
    "@babel/register": "7.0.0-beta.46",
    "@types/algoliasearch": "^3.27.1",
    "@types/bunyan": "^1.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.108",
    "@types/pg": "^7.4.8",
    "babel-plugin-module-resolver": "^3.0.0",
    "jest": "22.4.3",
    "ts-jest": "^22.4.6",
    "tsc": "^1.20150623.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "typescript-babel-jest": "^1.0.5"
  },
  "dependencies": {
    "algoliasearch": "^3.27.1",
    "bunyan": "^1.8.12",
    "dotenv": "^5.0.1",
    "lodash": "^4.17.10",
    "pg": "^7.4.3"
  }
}
